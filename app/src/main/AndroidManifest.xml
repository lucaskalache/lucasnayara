<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fcg1400019442.oboticario" >

    <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".AtividadePrincipal"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AtividadeDetalhes"
            android:label="@string/title_activity_atividade_detalhes"
            android:parentActivityName=".AtividadePrincipal" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.fcg1400019442.oboticario.AtividadePrincipal" />
        </activity>
    </application>

</manifest>
